<h1 class="edit-title">Editing Book</h1>
<%= form_with model:@book,local:true do |f| %>
<%= f.label:title,"Title" %>
<div>
<%= f.text_field:title %>
</div>
<%= f.label:body,"Body" %>
<div>
<%= f.text_area:body %>
</div>
<div>
<%= f.submit 'Update Book' %>
</div>
<% end %>
<%= link_to "Show", book_path(@book) %> |
<%= link_to "Back", books_path %>
